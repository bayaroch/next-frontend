    "@mui/styled-engine": "^6.1.4",
    "@reach/rect": "^0.18.0",
    "@react-oauth/google": "^0.12.1",
    "@react-spring/web": "^9.7.3",
    "@silevis/reactgrid": "^4.1.3",
    "axios": "^1.7.7",
    "browser-image-compression": "^2.0.2",
    "copy-to-clipboard": "^3.3.3",
    "customize-cra": "^1.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "history": "^5.3.0",
    "html-react-parser": "^5.0.7",
    "husky": "^8.0.3",
    "i18next": "^23.7.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-currency-input-field": "^3.9.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-facebook-login": "^4.1.1",
    "react-file-icon": "^1.3.0",
    "react-ga4": "^2.1.0",
    "react-google-forms-hooks": "^1.2.4",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "react-number-format": "^5.3.1",
    "react-phone-number-input": "^3.3.7",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.1",
    "secure-random-password": "^0.2.3",
    "standard-version": "^9.5.0",
    "web-vitals": "^3.5.0",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "dev": "start-test 3000 cy:open",
    "build": "react-app-rewired --max_old_space_size=8192 build",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "eject": "react-scripts eject",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install",
    "pretest:e2e:run": "npm run build",
    "fix-paths": "tspath -f",
    "check-translation": "ts-node ./src/locales/translationChecker.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run check-translation",
      "pre-push": "yarn run type-check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/customize-cra": "^1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.3",
    "@types/react": "^18",
    "@types/react-beautiful-dnd": "^13.1.7",
    "@types/react-big-calendar": "^1.8.7",
    "@types/react-dom": "^18",
    "@types/react-facebook-login": "^4.1.11",
    "@types/react-file-icon": "^1.0.4",
    "@types/react-fontawesome": "^1.6.8",
    "@types/react-html-parser": "^2.0.5",
    "@types/react-virtualized": "^9.21.29",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "start-server-and-test": "^2.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0"
  },
  "packageManager": "yarn@4.0.2"
}
